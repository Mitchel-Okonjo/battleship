{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": "off",
    "no-console": "off",
    "no-undef": "off",
    "no-use-before-define": "off",
    "no-plusplus": "off",
    "max-classes-per-file": "off",
    "class-methods-use-this": "off"
  }
}
